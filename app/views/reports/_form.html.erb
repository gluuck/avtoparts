<%= form_with(model: @report, local: true) do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :Клиент %>
  <%= form.text_field :customer %>
  <%= form.label :Работник %>
  <%= form.text_field :executor %>
  <%= form.label :Категория %>
  <%= form.text_field :category %>
  <%= form.label :Работы %>
  <%= form.text_field :service %></br>
  <%= form.submit 'Отчет', class: 'btn btn-small' %>
<% end %>
